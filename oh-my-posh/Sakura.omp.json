{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "diamond",
          "leading_diamond": "\u256d\u2500\ue0b6",
          "template": " {{ .Name }} ",
          "foreground": "#cbb6ff",
          "background": "#6964ab",
          "type": "shell"
        },
        {
          "properties": {
            "root_icon": "\uf292"
          },
          "style": "diamond",
          "template": "<parentBackground>\ue0b8</> \uf0e7 ",
          "foreground": "#f59574",
          "background": "#d52370",
          "type": "root"
        },
        {
          "properties": {
            "mapped_locations": {
              "C:\\Dev\\": "\ueef4",
              "C:\\Dev\\*": "\ueef4",
              "C:\\Dev\\Repos": "\ue725",
              "C:\\Dev\\Repos\\*": "\ue725",
              "C:\\Dev\\Repos\\Github": "\uf09b",
              "C:\\Dev\\Repos\\Github\\*": "\uf09b"
            },
            "style": "folder"
          },
          "style": "powerline",
          "template": " {{.Parent}}{{ .Path }} ",
          "foreground": "#18131e",
          "powerline_symbol": "\ue0b8",
          "background": "#939393",
          "type": "path"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{.UpstreamIcon}}{{ .HEAD }} {{if .BranchStatus }}{{ .BranchStatus }} {{ end }}{{ if .Working.Changed }}{{ .Working.String }} {{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }}|{{ end }}{{ if .Staging.Changed }}<#d52370> \uf046 {{ .Staging.String }}</> {{ end }}",
          "foreground": "#f59574",
          "powerline_symbol": "\ue0b8",
          "background": "#41af1a",
          "type": "git",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#41af1a{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#cbb6ff{{ end }}",
            "{{ if gt .Ahead 0 }}#cbb6ff{{ end }}",
            "{{ if gt .Behind 0 }}#cbb6ff{{ end }}"
          ],
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#f59574{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#9892f1{{ end }}",
            "{{ if gt .Ahead 0 }}#e90cdd{{ end }}",
            "{{ if gt .Behind 0 }}#e90cdd{{ end }}"
          ]
        },
        {
          "properties": {
            "always_enabled": true,
            "style": "austin"
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "template": " {{ .FormattedMs }} ",
          "foreground": "#cbb6ff",
          "background": "#6964ab",
          "type": "executiontime"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "style": "diamond",
          "leading_diamond": "\ue0ba",
          "trailing_diamond": "\ue0bc",
          "template": " \udb82\udeae {{ .Full }} ",
          "foreground": "#6964ab",
          "background": "#22e529",
          "type": "dotnet"
        },
        {
          "style": "diamond",
          "leading_diamond": "\ue0ba",
          "trailing_diamond": "\ue0bc",
          "template": " \ued4b {{ .Full }} ",
          "foreground": "#d52370",
          "background": "#22e529",
          "type": "angular"
        },
        {
          "style": "diamond",
          "leading_diamond": "\ue0ba",
          "trailing_diamond": "\ue0bc",
          "template": "{{ if .Error }}{{ else }} \ue7ba {{ .Full }} {{ end }}",
          "foreground": "#41af1a",
          "background": "#22e529",
          "type": "react"
        },
        {
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": "<#d52370>\ue5fa</>",
            "yarn_icon": "<#eeeeee>\ue6a7</>"
          },
          "style": "diamond",
          "leading_diamond": "\ue0ba",
          "trailing_diamond": "\ue0bc",
          "template": " {{if .PackageManagerIcon}}{{.PackageManagerIcon}}{{end}} \ue718 {{.Full}}{{if .Mismatch}} <#d52370>\uefcb {{.Expected}}</>{{end}} ",
          "foreground": "#f59574",
          "background": "#41af1a",
          "type": "node"
        },
        {
          "style": "diamond",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\uf019 {{ .Current }} -> {{ .Latest }}",
          "foreground": "#f59574",
          "background": "#d52370",
          "type": "upgrade"
        },
        {
          "style": "diamond",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue641 {{ .CurrentDate | date .Format }}",
          "foreground": "#18131e",
          "background": "#786d69",
          "type": "time",
          "invert_powerline": true
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": "\u2570\u2500",
          "foreground": "#9892f1",
          "background": "transparent",
          "type": "text"
        },
        {
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "‚ùØ ",
          "foreground": "#998eac",
          "background": "transparent",
          "type": "status",
          "foreground_templates": ["{{ if gt .Code 0 }}#d52370{{ end }}"]
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
